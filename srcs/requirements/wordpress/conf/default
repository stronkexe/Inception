server {
	listen					443 ssl;
	server_name				ael-asri;

	ssl_certificate			/etc/nginx/ssl/ael-asri.cert;
	ssl_certificate_key		/etc/nginx/ssl/ael-asri.key;
	ssl_protocols			TLSv1.2 TLSv1.3;

	root					/var/www/html;
	index					info.php index.html;

	location ~ \.php$ {
		include /etc/nginx/fastcgi.conf;
		fastcgi_pass 127.0.0.1:9000;
	}


  location ~ /\.ht {
    deny all;
  }
}